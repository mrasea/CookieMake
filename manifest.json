{
  "manifest_version": 3,
  "name": "CookieMake - Cookie管理器",
  "version": "1.0.0",
  "description": "一个用于管理、修改、删除cookie的Chrome插件",
  "permissions": [
    "cookies",
    "tabs",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Cookie管理器",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}